{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Robots-Tag", "value": "noindex, nofollow" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "has": [{ "type": "host", "value": "apply.arunatalent.com" }],
      "destination": "/apply.html"
    },
    {
      "source": "/referral",
      "destination": "/index.html"
    }
  ]
}
